<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.BlankCordovaApp1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="NfcPlugin">
        <param name="android-package" value="com.chariotsolutions.nfc.plugin.NfcPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>BlankCordovaApp1</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Ruben Alliet</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <vs:plugin name="org.apache.cordova.device-orientation" version="0.3.9" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <vs:plugin name="org.apache.cordova.network-information" version="0.2.12" />
    <vs:plugin name="com.chariotsolutions.nfc.plugin" version="0.6.1" />
    <vs:plugin name="org.apache.cordova.device" version="0.2.12" />
    <vs:platformSpecificValues />
</widget>
